@model IEnumerable<Perpustakaan.Models.Peminjaman>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TglPeminjaman)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoAnggotaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoBukuNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TglPeminjaman)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoAnggotaNavigation.NoAnggota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoBukuNavigation.NoBuku)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NoPeminjaman">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NoPeminjaman">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NoPeminjaman">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
